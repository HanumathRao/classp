class A {
  int a1[];
  syntax('<' a1 '>' * '|' '#');
}

class B {
  int a1[];
  int a2[];
  syntax('<' a1 ',' a2 '>' * '|');
}

class C {
  int a1[];
  int a2[];
  syntax('<' a1 ',' a2 '>' *);
}

class D {
  int a1[];
  int a2[];
  int a3[];
  int a4[];
  syntax(('<' a1 ',' a2 '>' +) ('<' a3 ',' a4 '>' + '|'));
}

class E {
  syntax(('<' '>' *)('<' '>' + ','));
}
