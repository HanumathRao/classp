class Pattern {
  %parseable;
  syntax('(' self ')');
}

class Iterator: Pattern {
  Pattern pattern1;
  Symbol separator;
  syntax(pattern1 '*' separator) %nassoc 2;
}

class Symbol : Pattern {
  identifier a;
  syntax(a);
}
